  "bugs": {
    "url": "https://github.com/rettersoft/retter-chess/issues"
  },
  "homepage": "https://github.com/rettersoft/retter-chess#readme",
  "devDependencies": {
    "aws-sdk": "2.1061.0",
    "@babel/preset-typescript": "7.16.7",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.9",
    "@types/sinon": "10.0.8",
    "jest": "27.4.7",
    "jsonschema": "1.4.0",
    "sinon": "12.0.1",
    "ts-jest": "27.1.3",
